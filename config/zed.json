{
  "agent": {
    "default_profile": "ask",
    "model_parameters": [],
    "default_model": {
      "provider": "vercel",
      "model": "v0-1.5-md"
    }
  },
  "show_edit_predictions": false,
  "autosave": "on_focus_change",
  "auto_install_extensions": {
    "github-theme": true
  },
  "buffer_font_size": 14,
  "buffer_font_family": "MesloLGS Nerd Font",
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "project_panel": {
    "auto_fold_dirs": false,
    "auto_reveal_entries": false
  },
  "terminal": {
    "dock": "right",
    "line_height": "standard"
  },
  "theme": {
    "mode": "dark",
    "dark": "Github Dark Dimmed",
    "light": "Github Light"
  },
  "language_models": {
    "openai": {
      "api_url": null
    }
  }
}
